{
  "name": "note-app",
  "version": "1.0.0",
  "description": "Note app for developer",
  "author": "Hieu Nguyen",
  "license": "ISC",
  "main": "dist/server.js",
  "scripts": {
    "tsc": "tsc",
    "client": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.dev.js",
    "server": "nodemon --config config/nodemon.config.json",
    "build": "",
    "check": "npm run prettier && npm run eslint",
    "eslint": "eslint -ext \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "eslint --fix --ext \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ntrhieu1905/note-app.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ntrhieu1905/note-app/issues"
  },
  "homepage": "https://github.com/ntrhieu1905/note-app#readme",
  "dependencies": {
    "antd": "^4.10.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dayjs": "^1.10.3",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "helmet": "^4.3.1",
    "http-status": "^1.5.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "multer": "^1.4.2",
    "mysql2": "^2.2.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.167",
    "@types/node": "^14.14.20",
    "@types/passport": "^1.0.5",
    "@types/passport-jwt": "^3.0.3",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "copy-webpack-plugin": "^7.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^4.5.1",
    "husky": "^4.3.7",
    "less": "^4.0.0",
    "less-loader": "^7.2.1",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "webpack": "^5.12.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-merge": "^5.7.3"
  }
}
